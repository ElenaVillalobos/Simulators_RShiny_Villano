n_sub <- 25            #i
n_que <- 24            #j
n_rep <- 10            #k
time_data <- NA
for (i in 1:n_sub){
time_data[i] <- paste('~/Google Drive/Nube/Intertemporal_Risky_Choice_Project/Tesis/Fase_buena/data/sujeto_',i,'_tiempo.csv',sep='')
}
subs_time <- lapply(time_data,read.csv)
pairs_time <- array(dim=c(n_sub,n_que,n_rep))
for(i in 1:n_sub){
for(j in 1:n_que){
pairs_time[i,j,] <- subset(subs_time[[i]][,7:8],pair==j)$biggerchosen
}
}
propo_time <- array(dim=c(n_sub,n_que))
for(i in 1:n_sub){
for(j in 1:n_que){
propo_time[i,j] <- sum(pairs_time[i,j,])/10
}
}
propo_time[,15] <- propo_time[,12]
propo_time[,13] <- propo_time[,6]
